   "source": [
    "#inverse mod 256 element\n",
    "import math\n",
    "def modulo_inverse(a):\n",
    "    if math.gcd(a,256) != 1:\n",
    "        return(\"does not exist\")\n",
    "    b = a%256\n",
    "    for i in range(1,256):\n",
    "        if ((b*i)%256 == 1):\n",
    "            return i\n",
    "            break\n",
    "\n",
    "print(modulo_inverse(8))"
   ]
  }
